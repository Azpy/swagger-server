{
  "name": "swagger-server",
  "version": "1.0.0-alpha-1",
  "description": "Get your REST API up-and-running FAST with Swagger and Express",
  "keywords": [
    "express",
    "swagger",
    "server",
    "mock",
    "fake",
    "stub",
    "test",
    "rest",
    "api",
    "json",
    "spec",
    "specification",
    "contract"
  ],
  "author": {
    "name": "James Messinger",
    "url": "http://jamesmessinger.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/BigstickCarpet/swagger-server",
  "main": "lib/index.js",
  "scripts": {
    "build": "jshint . --verbose && jscs . --verbose",
    "start": "node ./samples/petstore/server.js",
    "test": "istanbul cover _mocha -- --recursive tests",
    "mocha": "mocha --recursive tests",
    "update-deps": "npm-check-updates -u && npm update",
    "release": "npm run update-deps && npm run build && npm test && bump --prompt --tag --push --all && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BigstickCarpet/swagger-server.git"
  },
  "dependencies": {
    "debug": "^2.1.3",
    "express": "^4.12.3",
    "lodash": "^3.8.0",
    "swagger-express-middleware": "^0.4"
  },
  "devDependencies": {
    "chai": "^2.3.0",
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.13",
    "jscs": "^1.13.1",
    "jshint": "^2.7.0",
    "mocha": "^2.2.4",
    "nock": "^1.9.0",
    "npm-check-updates": "^1.5.1",
    "sinon": "^1.14.1",
    "supertest": "^0.15",
    "version-bump-prompt": "^1.1.0"
  }
}

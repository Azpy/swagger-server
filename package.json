{
  "name": "swagger-server",
  "version": "0.0.8",
  "description": "Get your REST API up-and-running FAST with Swagger and Express",
  "keywords": [
    "express",
    "swagger",
    "server",
    "mock",
    "fake",
    "stub",
    "test",
    "rest",
    "api",
    "json",
    "spec",
    "specification",
    "contract"
  ],
  "author": {
    "name": "James Messinger",
    "url": "http://jamesmessinger.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/BigstickCarpet/swagger-server",
  "main": "lib/index.js",
  "scripts": {
    "start": "node ./samples/petstore/server.js",

  "devDependencies": {
    "chai": "^2.1.2",
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.5",
    "mocha": "^2.0.1",
    "nock": "^1.2.0",
    "npm-check-updates": "^1.5.1",
    "sinon": "^1.12.2",
    "supertest": "^0.15",
    "version-bump-prompt": "^1.0.1"
  }
